# Generates the cmake find_package files for EXPutil

@PACKAGE_INIT@

if(NOT TARGET EXP::EXPutil)
    include(@PACKAGE_CMAKEPACKAGE_INSTALL_DIR@/EXPutilTargets.cmake)
endif()

set_and_check(EXPutil_INCLUDE_DIRS @PACKAGE_CMAKE_INSTALL_PREFIX@/include/exputil)
set_and_check(EXPutil_LIBRARY_DIRS @PACKAGE_CMAKE_INSTALL_PREFIX@/lib)

check_required_components(EXPutil)
