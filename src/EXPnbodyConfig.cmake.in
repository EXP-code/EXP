# Generates the cmake find_package files for EXPnbody

@PACKAGE_INIT@

if(NOT TARGET EXP::EXPnbody)
    include(@PACKAGE_CMAKEPACKAGE_INSTALL_DIR@/EXPnbodyTargets.cmake)
endif()

set_and_check(EXPnbody_INCLUDE_DIRS @PACKAGE_CMAKE_INSTALL_PREFIX@/include/expnbody)
